{
    "name": "build",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "rsbuild build",
        "postbuild": "cp -r dist/* static/",
        "daemon": "tsx daemon/server.ts",
        "worker:install": "npm install --prefix worker",
